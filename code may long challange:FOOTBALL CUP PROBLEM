#include <iostream>
using namespace std;

void solve(){
        int x,y;
        cin>>x>>y;
        
        if(x==y  && (x!=0 && y!=0)){
              cout<<"YES"<<endl;  
        }
        else{
                cout<<"NO"<<endl;
        }
}
int main() {
	// your code goes here
	
	int t;
	cin>>t;
	
	while(t--)
	{
	        solve();
	}
	return 0;
}
