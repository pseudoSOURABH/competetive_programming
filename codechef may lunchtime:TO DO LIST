#include <bits/stdc++.h>
#define ll long long
using namespace std;

void solve(){
        ll n;
        cin>>n;
        vector<ll>v(n);int count=0;
        
        for(int i=0;i<n;i++){
                cin>>v[i];
                if(v[i]>=1000)
                count++;
                
        }
        
        cout<<count<<endl;
        
        
}

int main() {
	// your code goes here
	ll t;
	cin>>t;
	
	while(t--)
	solve();
	return 0;
}
