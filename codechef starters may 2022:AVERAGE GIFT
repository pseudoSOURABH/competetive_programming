#include <bits/stdc++.h>
using namespace std;


void solve(){
        int n,x;
        cin>>n>>x;
        
        int mn=INT_MAX,mx=INT_MIN;
        
        vector<int>v(n);
        
        for(int i=0;i<n;i++)
        {
                cin>>v[i];
                mx=max(mx,v[i]);
                mn=min(mn,v[i]);
                
        }
        if(x>=mn && x<=mx)
        cout<<"YES"<<endl;
        
        else 
        cout<<"NO"<<endl;
        
        
        
}
int main() {
	// your code goes here
	
	int t;
	cin>>t;
	
	while(t--)
	solve();
	return 0;
}
