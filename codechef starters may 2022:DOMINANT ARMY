#include <iostream>
using namespace std;

void solve(){
        int a,b,c;
        
        cin>>a>>b>>c;
        
        int m=max(a,max(b,c));
        
        if(m>a+b || m>b+c || m>a+c)
        cout<<"YES"<<endl;
        else 
        cout<<"NO"<<endl;
}

int main() {
	// your code goes here
	
	int t;
	cin>>t;
	
	while(t--)
	solve();
	return 0;
}
